<div class="container">
  <h1>Selamat Datang Di Halaman Data Siswa</h1>
  <table class="table table-dark table-striped">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Nama Siswa</th>
        <th scope="col">Alamat</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
      <% banyakSiswa.forEach((siswa,i) => {i++ %>
      <tr>
        <th scope="row"><%= i++ %></th>
        <td><%= siswa.nama %></td>
        <td><%= siswa.alamat %></td>
        <td><%= siswa.email %></td>
      </tr>

      <% }) %>
    </tbody>
  </table>
</div>
